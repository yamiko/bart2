<html>
  <head>
    <title>Cohort Report</title>
    <style type="text/css">
      body {
        font-family: verdana;
        -moz-user-select: none;
      }
      .normalcell {
      	position:relative;
      	vertical-align: top;
        border: 1px solid #ccc;
      }
      .header {
        font-size: 28px;
      }
      .heading {
        border: 1px solid #ccc;
        font-weight: bold;
        font-size: 18px;
        color: #6281A7;
      }
      .num {
        text-align: center;
        min-width: 70px;
      }
      
      .initial {
      	width:60px;
      	font-weight:bold;
      	display:inline-flex;
      }
      
      .move_left {
      	margin-left:-32%;
      }
      .initial_ex {
      	width:96px;
      	font-weight:bold;
      	display:inline-flex;
      }
      
      .initial_d {
				padding-left: 200px;
				padding-right: 25px;
				width: 40px;
      	font-weight:bold;
      	text-align:left;
      	display:inline-flex;
      }
      
      .dataE {
      	display:block;
      	float:left;
      	text-align:center;
      	margin-right:3px;
      	margin-left:1px;
      	width:48%;
      }
      
      .line {
      	display:block;
      	width:2px;
      	float:left;
      	background-color:#CCCCCC;
      }
      
      #adult {
      	position:relative;
      	top:-48%;
      	left:0px;
      	display:block;
      }
      
      .textpos {
      	position:relative;
      	top:-100%;
      	left:0px;
      	width:40%;
      	display:block;      
      }

      .err-rows {
          background-color: red;
          color: white;
      }

      .pass-rows {
          background-color: green;
          color: black;
          border: 1px solid #000000;
      }

      .err-num {
          width: 3%;
      }
      
      #who {
      	display:block;
				width:auto;
				float:left;
				margin-top:25%;
      }
      
      #pshd {
      	display:block;
				float:left;
      }
      
      .verticalLine {
				-webkit-transform: rotate(270deg);
				-moz-transform: rotate(270deg);
				-o-transform: rotate(270deg);
				-ms-transform: rotate(270deg);
				transform: rotate(270deg);
      }

    </style>
  </head>
  <body>
    <table id="report" width="100%" cellspacing="2" cellpadding="5">
      <tr>
        <th style="background-color: #ccc;">
          &nbsp;
        </th>
        <th colspan="4" class="header" style="background-color: #6281A7;
            padding: 10px; color: #fff; font-weight: normal;">
          <span id="qtr">{quarter}</span> Cohort Report
          <br /><table style="width: 100%;" class="consistency-section"></table>
        </th>
      </tr>
      <tr>
        <th style="background-color: #ccc;">
          &nbsp;
        </th>
        <td id="site" class="normalcell header" style="color: #6281A7;">
          {site}
        </td>
        <td class="heading" style="color: #6281A7;">
          Newly Registered in Quarter
        </td>
        <td class="heading" colspan="2" style="color: #6281A7;">
          Cumulative Ever Registered
        </td>
      </tr>
      <tr>
        <th style="background-color: #ccc;">
          &nbsp;
        </th>
        <td class="heading" style="color: #6281A7;">
          Patient Registration Details
        </td>
        <th class="normalcell">
          <span class='dataE'> Clinic own data* </span> <span class='line'>&nbsp;</span><span class='dataE'> Checked data </span>
        </th>
        <th class="normalcell" colspan="2">
          <span class='dataE'> Clinic own data* </span> <span class='line'>&nbsp;</span><span class='dataE'> Checked data </span>
        </th>
      </tr>
      <tr>
        <td style="background-color: #ccc;">
          &nbsp;
        </td>
        <td class="normalcell">
          &nbsp;
        </td>
        <td class="normalcell">
          &nbsp;
        </td>
        <td class="normalcell" colspan="2">
          &nbsp;
        </td>
      </tr>
    </table>

