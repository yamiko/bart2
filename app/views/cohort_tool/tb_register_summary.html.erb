
<script>
	function load(){
    document.getElementById("print").innerHTML = "<span>Back</span>";
	  document.getElementById("print").setAttribute("onclick","history.back();")
	  document.getElementById("finishButton").setAttribute("onclick", "window.location= '/clinic'")

   }
	setTimeout("load();",30)
</script>

