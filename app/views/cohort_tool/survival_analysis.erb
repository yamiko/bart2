<html>
<head>
<%= javascript_include_tag "prototype" %>
<%= stylesheet_link_tag "cohort" %>
<script src="/javascripts/jquery-1.3.2.min.js" type="text/javascript"></script>
<script type="text/javascript">
  jQuery(document).ready(function($) {
        $('#switch_view').click(function() {
		  history.go(-1);
		})
		document.getElementById("switch_view").innerHTML = "<span>Back</span>";
  })

</script>
<script>
  var tt_cancel_destination = "/"
	switch_button = $('switch_view')
	switch_button.innerHTML = "<span>Cohort</span>"
</script>
<style>
  table {
    border-style:none;
    border-width:1px;
	position:absolute;
    top:200px;
    left:4%;
    margin-top:-40px;
  }

  th {
    border-style:none;
    border-width:1px;
  }

  td {
    text-align:center;
    border-style:solid;
    border-width:1px;
  }

  .row_break td {
    border-width:0px;
  }

  div.scrollTableContainer {
    border-style:solid;
  }

  .survival-analysis th, .survival-analysis td {
    text-align:left;
	display:inline;
  }

  .clinic_own_data {
		position:relative;
		float:left;
		display:block;
		padding-left:10px;
  }

  .checked_data {
		position:relative;
		float:right;
		display:block;
		padding-right:10px;
  }

  .clinic_data_value {
		position:relative;
		float:left;
		display:block;
		border: 0px 0px 2px 0px;
		border-top:0px;
		border-bottom:0px;
		border-left:0px;
		margin-top:2px;
		border-style:solid;
		width:40%;
		height:100%;
		padding-left:10px;
  }

  .checked_data_value {
		position:relative;
		float:right;
		display:block;
		border: 0px 0px 2px 0px;
		border-top:0px;
		border-bottom:0px;
		border-right:0px;
		border-left:0px;
		margin-top:2px;
		margin-top:2px;
		border-style:solid;
		width:41%;
		height:100%;
		padding-right:10px;
  }

	td.left_top_right {
		border-width:3px 3px 0px 3px;
	}

	td.right_bottom_left {
		border-width:1px 3px 3px 3px;
	}

	td.left_right {
		border-width:1px 3px 0px 3px;
	}

	td.left_top_right_bottom {
		border-width:3px 3px 3px 3px;
	}

	.regimen {
		position:absolute;
		top:0px;
		left:0px;
	}

	td.regimen_name_top {
		text-align:left;
		border-width:1px 0px 0px 0px;
		padding-left:100px;
	}

	td.regimen_name {
		text-align:left;
		border-width:0px 0px 0px 0px;
		padding-left:100px;
	}

	td.regimen_name_bottom {
		text-align:left;
		border-width:0px 0px 1px 0px;
		padding-left:100px;
	}

	b {
	  position:relative;
	  float:left;
	  display:block;
	  width:68px;
	  padding-left:5px;
	}

	td.bg {
   background-color:lightgrey;
   -moz-user-select:none;
   }
   .logo {
        float:left
      }

</style>

<title>Cohort<%= Location.current_health_center.name + " " + @quarter  %></title>
</head>

<body>
<%= image_tag("mw.gif",:class=>"logo")%>
		<br />
<h1><%= Location.current_health_center.name + " : " + @quarter %></h1>
<br />
<div class="survival_analysis">
<table align="left">
  
<% @survival_analysis.each do | key , values | %>
  <tr>
    <th style = "text-align:left;"><%=key%></th>
    <th>&nbsp;</th>
  </tr>
  <tr>
    <td style = "text-align:left;">New patients registered for ART</td>
    <td style = "text-align:right;"><%=values['New patients registered for ART'].length rescue 0%></td>
  </tr>
  <tr>
    <td style = "text-align:left;">Number Alive and on ART</td>
    <td style = "text-align:right;"><%=values['Number Alive and on ART'].length rescue 0%></td>
  </tr>
  <tr>
    <td style = "text-align:left;">Number Dead</td>
    <td style = "text-align:right;"><%=values['Number Dead'].length rescue 0%></td>
  </tr>
  <tr>
    <td style = "text-align:left;">Number Defaulted</td>
    <td style = "text-align:right;"><%=values['Unknown'].length rescue 0%></td>
  </tr>
  <tr>
    <td style = "text-align:left;">Number Stopped Treatment</td>
    <td style = "text-align:right;"><%=values['Number Stopped Treatment'].length rescue 0%></td>
  </tr>
  <tr>
    <td style = "text-align:left;">Number Transferred out</td>
    <td style = "text-align:right;"><%=values['Number Transferred out'].length rescue 0%></td>
  </tr>
  <tr>
    <td style = "text-align:left;">Number Unknown</td>
    <td style = "text-align:right;"><%=values['Unknown'].length rescue 0%></td>
  </tr>
<% end rescue nil%>
</table>
</div>
</body>
</html>